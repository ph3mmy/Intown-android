<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.intownexec.chat">
    <!--package="com.intownexec.chat">-->

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_CORSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.vending.BILLING" />

    <uses-feature android:name="android.hardware.location.gps" />

    <application
        android:name="com.intownexec.chat.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <meta-data
            android:name="android.app.default_searchable"
            android:value="com.intownexec.chat.SearchActivity" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.intownexec.chat.activity.SplashActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AppActivity" />

        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />



        <service
            android:name="com.intownexec.chat.service.MyFcmListenerService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.intownexec.chat.service.MyInstanceIDListenerService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.intownexec.chat.service.MyService"
            android:exported="false" />


        <activity
            android:name="com.intownexec.chat.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.SignupActivity"
            android:label="@string/title_activity_signup"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.MainActivity"
            android:label="@string/title_activity_feeds" />
        <activity
            android:name="com.intownexec.chat.RecoveryActivity"
            android:label="@string/title_activity_recovery"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name="com.intownexec.chat.WebViewActivity"
            android:label="@string/title_activity_web_view" />
        <activity
            android:name="com.intownexec.chat.ChangePasswordActivity"
            android:label="@string/title_activity_change_password"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.ServicesActivity"
            android:label="@string/title_activity_services" />
        <activity
            android:name="com.intownexec.chat.ProfileActivity"
            android:label="@string/title_activity_profile" />
        <activity
            android:name="com.intownexec.chat.NotificationsActivity"
            android:label="@string/title_activity_notifications" />
        <activity
            android:name="com.intownexec.chat.PhotoViewActivity"
            android:label="@string/title_activity_photo_view"
            android:theme="@style/PhotoViewTheme" />
        <activity
            android:name="com.intownexec.chat.LikesActivity"
            android:label="@string/title_activity_likes" />
        <activity
            android:name="com.intownexec.chat.AccountSettingsActivity"
            android:label="@string/title_activity_account_settings"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.BlackListActivity"
            android:label="@string/title_activity_black_list" />
        <activity
            android:name="com.intownexec.chat.DialogsActivity"
            android:label="@string/title_activity_dialogs" />
        <activity
            android:name="com.intownexec.chat.ChatActivity"
            android:label="@string/title_activity_chat"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.intownexec.chat.NotificationsSettingsActivity"
            android:label="@string/title_activity_notifications_settings" />
        <activity
            android:name="com.intownexec.chat.PrivacySettingsActivity"
            android:label="@string/title_activity_privacy_settings" />
        <activity
            android:name="com.intownexec.chat.SupportActivity"
            android:label="@string/title_activity_support"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.LocationActivity"
            android:label="@string/title_activity_location"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.DeactivateActivity"
            android:label="@string/title_activity_deactivate"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.AddPhotoActivity"
            android:label="@string/title_activity_new_item"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.GalleryActivity"
            android:label="@string/title_activity_gallery"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.SelectGiftActivity"
            android:label="@string/title_activity_select_gift"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.SendGiftActivity"
            android:label="@string/title_activity_send_gift"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.GiftsActivity"
            android:label="@string/title_activity_gifts"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.FriendsActivity"
            android:label="@string/title_activity_friends"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name="com.intownexec.chat.BalanceActivity"
            android:label="@string/title_activity_balance"
            android:screenOrientation="portrait"
            android:theme="@style/MyMaterialTheme">
        </activity>
        <activity
            android:name="com.intownexec.chat.PeopleNearbyActivity"
            android:label="@string/title_activity_nearby"
            android:theme="@style/MyMaterialTheme">

        </activity>
        <activity
            android:name="com.intownexec.chat.ViewImageActivity"
            android:label="@string/title_activity_view_gallery_item"
            android:theme="@style/MyMaterialTheme">
        </activity>
        <activity
            android:name="com.intownexec.chat.LikersActivity"
            android:label="@string/title_activity_likers" />
        <activity
            android:name="com.intownexec.chat.VideoViewActivity"
            android:label="@string/title_activity_video_view"
            android:theme="@style/PhotoViewTheme" />

        <activity android:name=".NewAppActivity"/>
        <activity android:name=".NewLoginActivity"/>
        <activity android:name=".activity.NewSignupActivity"/>

    </application>

</manifest>